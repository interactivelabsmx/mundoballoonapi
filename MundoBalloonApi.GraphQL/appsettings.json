{
  "EasyCaching": {
    "redis": {
      "MaxRdSecond": 120,
      "EnableLogging": false,
      "LockMs": 5000,
      "SleepMs": 300,
      "SerializerName": "json",
      "DBConfig": {
        "Password": null,
        "IsSsl": false,
        "SslHost": null,
        "ConnectionTimeout": 5000,
        "AllowAdmin": true,
        "Endpoints": [
          {
            "Host": "127.0.0.1",
            "Port": 6379
          }
        ],
        "Database": 1
      }
    },
    "AzureStorageConfig": {
      "AccountName": "",
      "AccountKey": "",
      "ImageContainer": "images",
      "ThumbnailContainer": "thumbnails"
    },
    "ClientRateLimiting": {
      "EnableEndpointRateLimiting": false,
      "StackBlockedRequests": false,
      "ClientIdHeader": "X-MundoB-ClientId",
      "HttpStatusCode": 429,
      "EndpointWhitelist": [
        "*:/api/status"
      ],
      "ClientWhitelist": [
        "dev.mundoballoon.com"
      ],
      "GeneralRules": [
        {
          "Endpoint": "*",
          "Period": "1s",
          "Limit": 10
        },
        {
          "Endpoint": "*",
          "Period": "15m",
          "Limit": 1000
        },
        {
          "Endpoint": "*",
          "Period": "12h",
          "Limit": 10000
        },
        {
          "Endpoint": "*",
          "Period": "7d",
          "Limit": 100000
        }
      ]
    }
  }
}
